;R = 1/(2*domain_size);

[p4est]
min_quadrants = 0
min_level = 1
fill_uniform = 1

[amr]
degmax = 7
amr_levels = 5
amr_inflation_size = 64
initial_degree = 2
percentile = 5
gamma_h = 2.0
gamma_p = 0.1


[flux]
ip_flux_penalty = 2.0

[problem]
rho0_div_rhoc = .01
domain_size = 5.0 
use_Gauss_quad = 1
deg_offset_for_Gauss_quad = 1

[solver]
ksp_type = cg
ksp_atol = 1e-20
ksp_rtol = 1e-10
ksp_maxit = 10000
ksp_view = 1
ksp_monitor = 1

[geometry]
name = cube